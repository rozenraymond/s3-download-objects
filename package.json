{
  "name": "s3-download-objects",
  "version": "1.0.0",
  "description": "Download all objects from a s3 bucket using Node.js",
  "scripts": {
    "clone": "node lib/index.js",
    "dev": "nodemon --exec babel-node src/index.js --ignore \"src/test\"",
    "build": "babel src --out-dir lib",
    "pretest": "./scripts/test-setup.sh",
    "test": "yarn run test:coverage",
    "posttest": "./scripts/test-cleanup.sh",
    "test:coverage": "jest --coverage",
    "format": "prettier --write '**/*.js'",
    "check:format": "prettier --check '**/*.js'",
    "check:lint": "eslint",
    "check": "yarn run check:format && yarn run check:lint"
  },
  "dependencies": {
    "aws-sdk": "^2.630.0",
    "bluebird": "^3.7.2",
    "convict": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "aws-sdk-mock": "^5.0.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "pretty-quick",
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rozenraymond/s3-download-objects.git"
  },
  "keywords": [
    "s3",
    "node"
  ],
  "author": "Rozen Raymond <rozenr.raymond@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rozenraymond/s3-download-objects/issues"
  },
  "homepage": "https://github.com/rozenraymond/s3-download-objects#readme"
}
